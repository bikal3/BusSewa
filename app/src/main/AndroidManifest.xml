<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bustracker">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        >
        <activity android:name=".detialui.NewsDetials"></activity>
        <activity android:name=".detialui.JobvacancyDetials" />
        <activity android:name=".detialui.BusRouteDetial" />
        <activity android:name=".ui.BusLocationsActivity" />
        <activity android:name=".ui.JobVacancyActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ui.BusRouteActivity" />
        <activity android:name=".ui.NewsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>